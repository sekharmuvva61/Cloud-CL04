drop table staging.congress_members
CREATE TABLE staging.congress_members(
	api_uri varchar(65),
	at_large varchar(10),
	contact_form varchar(10),
	crp_id varchar(10),
	cspan_id varchar(10),
	date_of_birth varchar(10),
	district varchar(10),
	dw_nominate varchar(10),
	facebook_account varchar(55),
	fax varchar(10),
	fec_candidate_id varchar(25),
	first_name varchar(25),
	gender varchar(5),
	geoid varchar(10),
	google_entity_id varchar(25),
	govtrack_id varchar(10),
	icpsr_id varchar(10),
	id varchar(10),
	ideal_point varchar(10),
	in_office varchar(10),
	last_name varchar(25),
	last_updated varchar(35),
	leadership_role varchar(55),
	middle_name varchar(25),
	missed_votes varchar(10),
	missed_votes_pct varchar(10),
	next_election varchar(10),
	ocd_id varchar(55),
	office varchar(55),
	party varchar(2),
	phone varchar(25),
	rss_url varchar(75),
	seniority varchar(10),
	short_title varchar(10),
	state varchar(10),
	suffix varchar(10),
	title varchar(25),
	total_present varchar(10),
	total_votes varchar(10),
	twitter_account varchar(25),
	url varchar(55),
	votes_against_party_pct varchar(10),
	votes_with_party_pct varchar(10),
	votesmart_id varchar(10),
	youtube_account varchar(25))

select * from staging.congress_members